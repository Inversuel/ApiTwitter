@model List<TwitterApiMVC.Models.RootHash>


@foreach (var item in Model)
{
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">@item.User.ScreenName</h5>
            <p class="card-text">@item.Text</p>
            <p class="card-text"><small class="text-muted">Created at : @item.CreatedAt </small></p>
        </div>
    </div>

}

